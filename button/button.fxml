<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.Group?>

<Group xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Button">
<AnchorPane style="-fx-border-color: green; -fx-border-width: 20; -fx-border-radius: 200; -fx-background-color: #00000088; -fx-background-radius: 200;">
   <children>
      <Button layoutX="177.0" layoutY="173.0" mnemonicParsing="false" onAction="#stop" text="Stop">
         <font>
            <Font name="System Bold" size="92.0" />
         </font>
         <effect>
            <Reflection />
         </effect>
      </Button>
   </children>
   <padding>
      <Insets bottom="100.0" left="100.0" right="100.0" top="100.0" />
   </padding>
   <effect>
      <DropShadow color="#000000eb" offsetX="8" offsetY="8"/>
   </effect>
</AnchorPane>
</Group>
